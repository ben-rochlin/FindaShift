<section class="jumbotron">
  <div class="container text-center">
    <h1 class="display-4">FindaShift Login</h1>
  </div>
</section>


<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>



<section>
  <div class="container text-center">
    <div class="row justify-content-center">
      <div class="col-md-5 col-lg-4">




        <label class="sr-only" for="input1-signin-01">Email</label>

        <%= f.email_field :email, autofocus: true, autocomplete: "email", class: "form-control my-3 bg-light" %>

        <%# <input class="form-control my-3 bg-light" id="input1-signin-01" type="email" placeholder="Email"> %>





        <label class="sr-only" for="input2-signin-01">Password</label>

         <%= f.password_field :password, autocomplete: "Password", class: "form-control my-3 bg-light"  %>

        <%# <input class="form-control my-3 bg-light" id="input2-signin-01" type="password" placeholder="Password"> %>





        <%# <button class="btn btn-primary btn-block py-2 my-3">Sign in!</button> %>
        <%= f.submit "Login", class: "actions btn btn-primary btn-block py-2 my-3"  %>

        <%= render "devise/shared/links" %>
      </div>
    </div>
  </div>
</section>

<% end %>


